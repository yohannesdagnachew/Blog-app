<div class='post-container'>
  <h5 class='post-container--heading'><%= link_to 'Post #', user_post_path(current_user_id, post_data.id) %><%=post_data.id%></h5>
  <p class='post-container--text'><%=post_data.text%></p>
  <p class='post-container--counter'>
    Comments: <%=post_data.comment_counter%>, Likes: <%=post_data.like_counter%>
  </p>
  <%= flash[:success]%>
</div>
